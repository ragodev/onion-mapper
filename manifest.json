{
	"name": "Onion Mapper",
	"version": "0.1",
	"description": "Redirect websites to their onion address",
	"background": {
		"scripts": ["background.js"]
	},
	"icons": {
		"128": "icon.png"
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"manifest_version": 2,
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"tabs",
		"<all_urls>"
	]
}
